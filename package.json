{
  "name": "poe",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "main": "dist/entry.js",
  "scripts": {
    "ng": "ng",
    "start": "concurrently -r -k -s first \"yarn api\" \"yarn serve\" \"yarn electron\"",
    "serve": "ng server -p 4848 -pc proxy.conf.json",
    "api": "json-server -p 3030 -w db.json",
    "build": "ng build --prod",
    "electron": "electron src/entry.dev.js",
    "electron:prod": "yarn build && electron ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "core-js": "^2.4.1",
    "electron": "^1.7.11",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular-devkit/core": "^0.0.28",
    "@angular/cli": "1.6.2",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "concurrently": "^3.5.1",
    "json-server": "^0.12.1",
    "typescript": "~2.4.2"
  }
}
